    <!-- Sync fragment -->
    <string name="sync_with_sync_app_heading">"Synchronizovat s %1$s"</string>
    <string name="sync_basic_info">"jtx Board sám o sobě neposkytuje synchronizaci se serverem, ale podporuje synchronizaci prostřednictvím DAVx⁵. Tato metoda vám umožňuje používat vámi vybraný jakýkoliv server kompatibilní s CalDAV k ukládání, zálohování a synchronizaci dat."</string>
    <string name="sync_ksync_basic_info">"kSync je aplikace založená na DAVx5 vyvinutá Infomaniak zejména pro jejich služby. Zatímco DAVx5 je otevřen libovolnému kompatibilnímu serveru CalDAV, kSync umožňuje synchronizaci pouze pro účty Infomaniak."</string>
    <string name="sync_manageddavx5_basic_info">"Spravované DAVx⁵ zpřístupňuje funkce DAVx⁵ organizacím a přichází s dalšími podnikovými funkcemi, jako je hromadné nasazení, firemní značka a přednastavitelné nastavení."</string>
