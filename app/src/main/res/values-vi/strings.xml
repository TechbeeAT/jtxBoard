    <!-- Sync fragment -->
    <string name="sync_with_sync_app_heading">"Đồng bộ hóa với %1$s"</string>
    <string name="sync_basic_info">"jtx Board không tự cung cấp tính năng đồng bộ hóa máy chủ nhưng hỗ trợ đồng bộ hóa thông qua DAVx⁵. Phương pháp này cho phép bạn sử dụng bất kỳ máy chủ CalDAV tương thích nào mà bạn chọn để lưu trữ, sao lưu và đồng bộ hóa dữ liệu của mình."</string>
    <string name="sync_ksync_basic_info">"kSync là một ứng dụng dựa trên DAVx⁵ được Infomaniak phát triển đặc biệt cho các dịch vụ của họ. Mặc dù DAVx⁵ mở cho mọi máy chủ CalDAV tương thích nhưng kSync chỉ cho phép đồng bộ hóa các tài khoản Infomaniak."</string>
    <string name="sync_manageddavx5_basic_info">"DAVx⁵ được quản lý cung cấp chức năng của DAVx⁵ cho các tổ chức và đi kèm với các tính năng bổ sung dành cho doanh nghiệp như triển khai hàng loạt, xây dựng thương hiệu công ty và các cài đặt có thể định cấu hình trước."</string>
    <string name="sync_congratulations">"Chúc Mừng! :-)"</string>
